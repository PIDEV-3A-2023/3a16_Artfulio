<form method="post" action="{{ path('app_service_delete', {'idService': service.idService}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service.idService) }}">
    <button class="btn btn-link"><i class="bi bi-trash"></i></button>
</form>



