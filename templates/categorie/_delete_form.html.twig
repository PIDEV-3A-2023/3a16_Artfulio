<form method="post" action="{{ path('app_categorie_delete', {'id_categorie': categorie.id_categorie}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.id_categorie) }}">
    <button class="btn">Delete</button>
</form>
